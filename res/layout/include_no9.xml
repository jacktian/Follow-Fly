<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/include_no9"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@id/im_btn_jpg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place"
            android:layout_marginLeft="2dp" />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" 
            android:layout_marginLeft="2dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:orientation="horizontal" >

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" 
            android:layout_marginLeft="2dp" />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place"
            android:layout_marginLeft="2dp"  />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:orientation="horizontal" >

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place"
            android:layout_marginLeft="2dp"  />

        <com.suixin.vy.core.SquareImageView
            android:id="@+id/Iv_no9_7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:scaleType="centerCrop"
            android:src="@drawable/default_place" 
            android:layout_marginLeft="2dp" />
    </LinearLayout>

</LinearLayout>