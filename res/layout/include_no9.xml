<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/include_no9"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@id/im_btn_jpg" >

    <ImageView
        android:id="@+id/Iv_no9_1"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_2"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_3"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_5"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/Iv_no9_3"
        android:layout_alignStart="@+id/Iv_no9_3"
        android:layout_below="@+id/Iv_no9_3"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_9"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/Iv_no9_5"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_8"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/Iv_no9_5"
        android:layout_alignStart="@+id/Iv_no9_5"
        android:layout_alignTop="@+id/Iv_no9_9"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_7"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/Iv_no9_8"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_6"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/Iv_no9_5"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

    <ImageView
        android:id="@+id/Iv_no9_4"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_above="@+id/Iv_no9_9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/default_place" />

</RelativeLayout>