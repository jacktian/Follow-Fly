<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/include_no6"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:contentDescription="@string/hello_world"
        android:id="@+id/Iv_no6_1"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:src="@drawable/bg_user2" />

    <ImageView
        android:contentDescription="@string/hello_world"
        android:id="@+id/Iv_no6_2"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/bg_user2" />

    <ImageView
        android:id="@+id/Iv_no6_3"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/bg_user2" />

    <ImageView
        android:id="@+id/Iv_no6_4"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/Iv_no6_6"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/bg_user2" />

    <ImageView
        android:id="@+id/Iv_no6_6"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/Iv_no6_5"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/bg_user2" />

    <ImageView
        android:id="@+id/Iv_no6_5"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/Iv_no6_3"
        android:layout_below="@+id/Iv_no6_3"
        android:layout_marginTop="5dp"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/bg_user2" />

</RelativeLayout>